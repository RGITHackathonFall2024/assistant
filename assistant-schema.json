{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Assistant Response",
  "required": [
    "follow_up",
    "response",
    "function_calls"
  ],
  "properties": {
    "follow_up": {
      "type": "boolean",
      "title": "Follow Up",
      "description": "Indicates if a function response is expected"
    },
    "response": {
      "type": "array",
      "title": "Response Messages",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "Текстовое сообщение",
            "description": "Базовое текстовое сообщение с поддержкой Markdown",
            "required": [
              "type",
              "text"
            ],
            "properties": {
              "type": {
                "type": "string",
                "description": "Указывает тип сообщения как текст, всегда должно быть \"text\"",
                "title": "Type"
              },
              "text": {
                "type": "string",
                "description": "Текст в формате Markdown",
                "title": "Text"
              }
            }
          },
          {
            "type": "object",
            "title": "Сообщение кнопка с ссылкой",
            "description": "Кнопка имеющая текст и ведущая на другой сайт",
            "required": [
              "type",
              "text",
              "url"
            ],
            "properties": {
              "type": {
                "type": "string",
                "description": "Указывает тип сообщения как кнопка, всегда должно быть \"button_url\"",
                "title": "Type"
              },
              "text": {
                "type": "string",
                "description": "Текст кнопки",
                "title": "Text"
              },
              "url": {
                "type": "string",
                "description": "Ссылка куда кнопка ведёт",
                "title": "Url"
              }
            }
          }
        ]
      }
    },
    "function_calls": {
      "type": "array",
      "title": "Function Calls",
      "items": {
        "type": "object",
        "required": [
          "namespace",
          "name",
          "args"
        ],
        "properties": {
          "namespace": {
            "type": "string",
            "enum": [
              "rzdtrains",
              "realEstate"
            ]
          },
          "name": {
            "type": "string",
            "enum": [
              "queryStations",
              "queryTickets",
              "getListings",
              "getListingById"
            ]
          },
          "args": {
            "type": "object"
          }
        }
      }
    }
  },
  "$defs": {
    "Текстовоесообщение": {
      "type": "object",
      "title": "Текстовое сообщение",
      "description": "Базовое текстовое сообщение с поддержкой Markdown",
      "required": [
        "type",
        "text"
      ],
      "properties": {
        "type": {
          "type": "string",
          "description": "Указывает тип сообщения как текст, всегда должно быть \"text\"",
          "title": "Type"
        },
        "text": {
          "type": "string",
          "description": "Текст в формате Markdown",
          "title": "Text"
        }
      }
    },
    "Сообщениекнопкасссылкой": {
      "type": "object",
      "title": "Сообщение кнопка с ссылкой",
      "description": "Кнопка имеющая текст и ведущая на другой сайт",
      "required": [
        "type",
        "text",
        "url"
      ],
      "properties": {
        "type": {
          "type": "string",
          "description": "Указывает тип сообщения как кнопка, всегда должно быть \"button_url\"",
          "title": "Type"
        },
        "text": {
          "type": "string",
          "description": "Текст кнопки",
          "title": "Text"
        },
        "url": {
          "type": "string",
          "description": "Ссылка куда кнопка ведёт",
          "title": "Url"
        }
      }
    },
    "rzdtrains_queryStations_params": {},
    "rzdtrains_queryStations_returns": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "expressCode": {
            "type": "string"
          },
          "region": {
            "type": "string"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "expressCode",
          "region",
          "name"
        ]
      }
    },
    "rzdtrains_queryTickets_params": {},
    "rzdtrains_queryTickets_returns": {
      "type": "object",
      "properties": {
        "trains": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "display_name": {
                "type": "string"
              },
              "url": {
                "type": "string"
              },
              "origin": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "code": {
                    "type": "string"
                  }
                },
                "required": [
                  "name",
                  "code"
                ]
              },
              "destination": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "code": {
                    "type": "string"
                  }
                },
                "required": [
                  "name",
                  "code"
                ]
              },
              "prices": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "min_price": {
                      "type": "number"
                    },
                    "max_price": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "type",
                    "min_price",
                    "max_price"
                  ]
                }
              },
              "duration": {
                "type": "string"
              },
              "distance": {
                "type": "number"
              },
              "departure": {
                "type": "object",
                "properties": {
                  "local": {
                    "type": "string"
                  },
                  "normal": {
                    "type": "string"
                  }
                },
                "required": [
                  "local",
                  "normal"
                ]
              },
              "arrival": {
                "type": "object",
                "properties": {
                  "local": {
                    "type": "string"
                  },
                  "normal": {
                    "type": "string"
                  }
                },
                "required": [
                  "local",
                  "normal"
                ]
              }
            },
            "required": [
              "display_name",
              "url",
              "origin",
              "destination",
              "prices",
              "duration",
              "distance",
              "departure",
              "arrival"
            ]
          }
        },
        "url": {
          "type": "string",
          "description": "Ссылка для просмотра всех билетов"
        }
      },
      "required": [
        "trains",
        "url"
      ]
    },
    "realEstate_getListings_params": {},
    "realEstate_getListings_returns": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
          "description": "Список объявлений, соответствующих фильтрам",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "integer",
                "description": "Уникальный идентификатор объявления"
              },
              "title": {
                "type": "string",
                "description": "Название объявления с кратким описанием объекта"
              },
              "description": {
                "type": "string",
                "description": "Полное описание объекта недвижимости"
              },
              "address": {
                "type": "string",
                "description": "Адрес объекта недвижимости"
              },
              "area": {
                "type": "string",
                "description": "Район, в котором расположен объект"
              },
              "rooms": {
                "type": "integer",
                "description": "Количество комнат"
              },
              "price": {
                "type": "number",
                "description": "Цена объекта недвижимости"
              },
              "location": {
                "type": "object",
                "description": "Географические координаты объекта",
                "properties": {
                  "lat": {
                    "type": "number",
                    "description": "Широта"
                  },
                  "lng": {
                    "type": "number",
                    "description": "Долгота"
                  }
                },
                "required": [
                  "lat",
                  "lng"
                ]
              },
              "details": {
                "type": "object",
                "description": "Подробные характеристики объекта",
                "properties": {
                  "floor": {
                    "type": "integer",
                    "description": "Этаж"
                  },
                  "totalFloors": {
                    "type": "integer",
                    "description": "Общее количество этажей в здании"
                  },
                  "area": {
                    "type": "number",
                    "description": "Площадь объекта в квадратных метрах"
                  },
                  "hasBalcony": {
                    "type": "boolean",
                    "description": "Наличие балкона"
                  },
                  "hasParking": {
                    "type": "boolean",
                    "description": "Наличие парковки"
                  },
                  "renovationType": {
                    "type": "string",
                    "description": "Тип ремонта"
                  }
                },
                "required": [
                  "floor",
                  "totalFloors",
                  "area",
                  "hasBalcony",
                  "hasParking",
                  "renovationType"
                ]
              },
              "photos": {
                "type": "array",
                "description": "Массив ссылок на фотографии объекта",
                "items": {
                  "type": "string"
                }
              },
              "contact": {
                "type": "object",
                "description": "Контактная информация владельца объявления",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Имя контактного лица"
                  },
                  "phone": {
                    "type": "string",
                    "description": "Телефонный номер контактного лица"
                  },
                  "email": {
                    "type": "string",
                    "description": "Электронная почта контактного лица"
                  }
                },
                "required": [
                  "name",
                  "phone",
                  "email"
                ]
              },
              "created": {
                "type": "string",
                "description": "Дата и время создания объявления"
              },
              "views": {
                "type": "integer",
                "description": "Количество просмотров объявления"
              },
              "isFavorite": {
                "type": "boolean",
                "description": "Является ли объявление избранным"
              }
            },
            "required": [
              "id",
              "title",
              "description",
              "address",
              "area",
              "rooms",
              "price",
              "location",
              "details",
              "photos",
              "contact",
              "created",
              "views",
              "isFavorite"
            ]
          }
        },
        "total": {
          "type": "integer",
          "description": "Общее количество объявлений, соответствующих фильтрам"
        },
        "page": {
          "type": "integer",
          "description": "Текущий номер страницы"
        },
        "totalPages": {
          "type": "integer",
          "description": "Общее количество страниц"
        }
      },
      "required": [
        "items",
        "total",
        "page",
        "totalPages"
      ]
    },
